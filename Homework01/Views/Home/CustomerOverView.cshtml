@model IEnumerable<Homework01.Models.vw_CustomerOverView>

@{
    ViewBag.Title = "CustomerOverView";
}

<h2>客戶資訊總覽</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.客戶名稱)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BankTotal)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContactTotal)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.客戶名稱)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BankTotal)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ContactTotal)
        </td>
    </tr>
}

</table>
